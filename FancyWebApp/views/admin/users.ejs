<% if(typeof users != 'undefined') { %>
<h4>
    Administratörer<div class="chip" style="
    display: inline-block;
    margin: 0 0 5px 25px;
    height: 24px;
    line-height: 24px;
    border-radius: 2px;
    position: relative;
    bottom: 5px;
">Visa alla
</h4>
<ul class="collection">
    <% users.forEach(function(user) { if(user.userlevel == 3) { %>
    <li class="collection-item">
        <a href="/users/<%= user._id %>"><%= user.name %></a>
    </li>
    <% }}); %>
</ul>

